{
  "name": "@ilemandatow/api",
  "version": "0.0.1-alpha.7",
  "description": "API of ilemandatow",
  "author": "Micha≈Ç Malik",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build:keystone": "yarn keystone build",
    "build": "yarn cleanup && tsc",
    "check-types": "tsc --noEmit",
    "cleanup": "rimraf dist && rimraf build",
    "db:seed": "knex seed:run",
    "db:setup": "keystone create-tables && yarn knex migrate:up",
    "db:migrate:up": "yarn knex migrate:up",
    "db:migrate:down": "yarn knex migrate:down",
    "develop": "yarn build && NODE_ENV=development keystone",
    "start": "NODE_ENV=production keystone start"
  },
  "dependencies": {
    "@keystonejs/adapter-knex": "^13.0.0",
    "@keystonejs/app-admin-ui": "^7.3.13",
    "@keystonejs/app-graphql": "^6.2.1",
    "@keystonejs/auth-password": "^6.0.0",
    "@keystonejs/fields": "^21.0.2",
    "@keystonejs/fields-color": "^1.0.9",
    "@keystonejs/keystone": "^18.1.0",
    "bcryptjs": "^2.4.3",
    "connect-pg-simple": "^6.2.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "knex": "^0.21.16",
    "nanoid": "^3.1.20",
    "postgres": "^1.0.2",
    "slugify": "^1.4.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-pg-simple": "^4.2.2",
    "@types/express-session": "^1.17.3"
  }
}
