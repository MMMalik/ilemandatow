{
  "name": "@ilemandatow/ilemandatow",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/site",
      "packages/seats",
      "packages/cms"
    ]
  },
  "scripts": {
    "build": "lerna run build",
    "clean": "lerna run clean",
    "lint": "eslint \"packages/**/src/**/*.{tsx,ts,js}\"",
