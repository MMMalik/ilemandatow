{
  "name": "@ilemandatow/ilemandatow",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "build": "lerna run build --stream",
    "build:api:keystone": "cd packages/api && yarn build:keystone",
    "check-types": "lerna run check-types --stream",
    "clean": "lerna run clean --stream",
    "develop:api": "cd packages/api && yarn develop",
    "develop:cms": "cd packages/cms && yarn develop",
    "develop:seats": "cd packages/seats && yarn build:watch",
    "develop:site": "cd packages/site && yarn develop",
    "develop:ui:sb": "cd packages/ui && yarn storybook",
    "develop:ui": "cd packages/ui && yarn build:watch",
    "jest:watch": "yarn jest --watch",
    "lint": "eslint \"packages/**/src/**/*.{tsx,ts,js}\"",
    "serve:cms": "cd packages/cms && yarn serve",
    "serve:site": "cd packages/site && yarn serve",
    "test": "yarn check-types && yarn jest && yarn lint"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "netlify-cli": "^2.70.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2"
  }
}
